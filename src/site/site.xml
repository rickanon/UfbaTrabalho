<project xmlns="http://maven.apache.org/DECORATION/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0
                             http://maven.apache.org/xsd/decoration-1.0.0.xsd"
         name="RiVal Recommender System Evaluation">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.1</version>
    </skin>
    <publishDate position="right"/>
    <version position="right"/>
    <poweredBy/>
    <bannerLeft>
        <name>RiVal</name>
        <src>logo.png</src>
        <href>http://rival.recommenders.net/</href>
    </bannerLeft>
    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>recommenders/rival</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>
    <body>
        <breadcrumbs>
            <item name="recommenders.net"
                  href="http://www.recommenders.net/"/>
            <item name="RiVal"
                  href="index.html"/>
        </breadcrumbs>
        <menu name="RiVal" inherit="top">
            <item name="Project Home" href="http://rival.recommenders.net/"/>
            <item name="GitHub" href="http://github.com/recommenders/rival"/>
        </menu>
        <menu name="Documentation">
            <item name="Getting Started" href="http://github.com/recommenders/rival/wiki/GettingStarted"/>
            <!--item name="Manual" href="http://github.com/recommenders/rival/wiki/Manual"/-->
            <item name="Release Notes" href="releases"/>
            <item name="JavaDoc" href="apidocs"/>
        </menu>
        <menu ref="modules"/>
        <menu name="Module Info" ref="reports" inherit="bottom"/>
    </body>
</project>
